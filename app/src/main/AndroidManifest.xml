<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.study">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_ball_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Study">
        <activity
            android:name=".RateActivity"
            android:label="@string/title_activity_rate"
            android:theme="@style/Theme.Study.NoActionBar"></activity>
        <activity
            android:name=".ball"
            android:label="@string/title_activity_ball"
            android:theme="@style/Theme.Study.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Study.NoActionBar"></activity>
    </application>

</manifest>